<launch>
    <group ns="j1">
        <node name="ds4_joystick" pkg="joy" type="joy_node">
                <param name="dev" value="/dev/input/js1" />
        </node>
    </group>
   <node pkg="robocop_description" name="ps4Controller" type="ps4Controller.py" output = "screen"/> 
   <node pkg="rosserial_python" name="arduino_node" type="serial_node.py" output = "screen">
        <param name="port" value="/dev/ttyACM0" />
        <param name="baud" value="115200" />
   </node>
</launch>
